  var int = self.setInterval("varupdate()", 250);

  function varupdate() {
    if (window.XMLHttpRequest) { //code for IE7+, FF,Chr, Opera, Safari
      xmlhttp = new XMLHttpRequest();
    } else {
      xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    }
